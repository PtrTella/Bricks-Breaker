<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="root" minHeight="463.0" minWidth="630.0" style="-fx-background-color: black;"
   xmlns="http://javafx.com/javafx/15" xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="brickbreaker.view.HomeView">
   <children>
      <VBox fx:id="vbAnchor" alignment="CENTER" layoutX="-2.0" layoutY="-1.0" prefHeight="264.0"
         prefWidth="560.0" spacing="30.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
         AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fx:id="imgTitle" fitHeight="85.0" fitWidth="364.0" pickOnBounds="true"
               preserveRatio="true">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </ImageView>
            <VBox fx:id="vbButtons" alignment="CENTER_LEFT" prefHeight="229.0" prefWidth="600.0"
               spacing="20.0">
               <children>
                  <ImageView fx:id="imgLevel" fitHeight="68.0" fitWidth="354.0"
                     onMouseClicked="#switchToLevel" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets left="120.0" />
                     </VBox.margin>
                  </ImageView>
                  <ImageView fx:id="imgEndless" fitHeight="68.0" fitWidth="354.0"
                     onMouseClicked="#switchToEndless" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets left="120.0" />
                     </VBox.margin>
                  </ImageView>
                  <ImageView fx:id="imgLeaderboards" fitHeight="68.0" fitWidth="354.0"
                     onMouseClicked="#switchToLeaderboards" pickOnBounds="true" preserveRatio="true"
                     VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets left="120.0" />
                     </VBox.margin>
                  </ImageView>
                  <ImageView fx:id="imgBack" fitHeight="68.0" fitWidth="354.0"
                     onMouseClicked="#clickBack" pickOnBounds="true" preserveRatio="true"
                     VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets left="120.0" />
                     </VBox.margin>
                  </ImageView>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>